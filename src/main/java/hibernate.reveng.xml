<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-name=".*"/>
  <table-filter match-catalog="fishshop" match-name="admin"/>
  <table-filter match-catalog="fishshop" match-name="catalog"/>
  <table-filter match-catalog="fishshop" match-name="order"/>
  <table-filter match-catalog="fishshop" match-name="product"/>
  <table-filter match-catalog="fishshop" match-name="transaction"/>
  <table-filter match-catalog="fishshop" match-name="user"/>
</hibernate-reverse-engineering>